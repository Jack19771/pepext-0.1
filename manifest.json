{
  "manifest_version": 3,
  "name": "Peplink WiFi Helper",
  "version": "1.0",
  "description": "Minimalistic extension to help with Peplink WiFi settings",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://192.168.50.1/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://192.168.50.1/*"],
      "js": ["content.js"]
    }
  ]

}
